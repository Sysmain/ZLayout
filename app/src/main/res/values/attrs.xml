<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ZLayout">
        <attr name="lineSpacing" format="dimension" />
        <attr name="maxLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="TagTextView">
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TagLayout">
        <attr name="checkedType" format="enum">
            <enum name="single" value="0" />
            <enum name="multiple" value="1" />
        </attr>

    </declare-styleable>

</resources>
